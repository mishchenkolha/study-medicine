{
  "kind": "collectionType",
  "collectionName": "quizzes",
  "info": {
    "singularName": "quiz",
    "pluralName": "quizzes",
    "displayName": "quiz"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "toolbarBalloon"
      }
    },
    "questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::question.question"
    },
    "min_percent_treshold": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 100
    },
    "course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course.course"
    }
  }
}
