{
  "kind": "collectionType",
  "collectionName": "results",
  "info": {
    "singularName": "result",
    "pluralName": "results",
    "displayName": "result"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course.course"
    },
    "quiz": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::quiz.quiz"
    },
    "score": {
      "type": "decimal"
    },
    "submittedAt": {
      "type": "datetime"
    },
    "answers": {
      "type": "json"
    },
    "passed": {
      "type": "boolean",
      "default": false
    },
    "certificate_sent": {
      "type": "boolean",
      "default": false
    }
  }
}
