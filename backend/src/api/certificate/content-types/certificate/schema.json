{
  "kind": "collectionType",
  "collectionName": "certificates",
  "info": {
    "singularName": "certificate",
    "pluralName": "certificates",
    "displayName": "certificate"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "course": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::course.course"
    },
    "certificate": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files"
      ]
    }
  }
}
